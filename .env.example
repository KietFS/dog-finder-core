# Application
NODE_ENV=development
PORT=3000
API_PREFIX=api/v1

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=dog_finder_db
DB_SYNCHRONIZE=false
DB_LOGGING=true

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_TTL=3600

# Swagger
SWAGGER_ENABLED=true
SWAGGER_TITLE=Dog Finder API
SWAGGER_DESCRIPTION=Production-ready RESTful API that mimics TheDogAPI
SWAGGER_VERSION=1.0
SWAGGER_PATH=api

# Pagination
DEFAULT_PAGE_SIZE=10
MAX_PAGE_SIZE=100
